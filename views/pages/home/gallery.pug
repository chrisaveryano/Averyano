figure.home__gallery__item(id="g1") 
	figcaption.home__gallery__item__top
		h3.home__gallery__item__title='Photographers for Ukraine'
		h6.home__gallery__item__subtitle!=`Buy <strong>art</strong> â€” give <strong>support</strong>!`.replace(/<strong>/g, '<strong>')
	.home__gallery__item__content
		.home__gallery__video__container
			.video__controls__wrapper
				img.video__controls.video__controls--play(data-src="./images/play.svg")
				img.video__controls.video__controls--pause(data-src="./images/paused.svg")
				div.video__controls.video__controls--progressbar
				button.video__controls.video__controls--fullscreen="Fullscreen"
			video.home__gallery__item__video(width="640" height="360" data-src="./images/prj1preview.jpg" playsinline loop auto muted)
				source(data-src="./videos/phforukr.mp4" type="video/mp4")
	.home__gallery__item__bottom
		img.home__gallery__item__icon(data-src="./images/pbox.png" width="16" height="16" title="made with Portfoliobox" alt="portfoliobox")
		a.home__gallery__item__link(href='https://www.photographersforukraine.com/' target="_blank")='photographersforukraine.com'

